CREATE OR REPLACE TABLE CANX_REASON (canx_reason_code VARCHAR PRIMARY KEY,
                          canx_reason VARCHAR,
                          canx_abbrev VARCHAR)
                    WITH (KAFKA_TOPIC='CANX_REASON_CODE',
                    VALUE_FORMAT='JSON');

CREATE TABLE CANX_REASON6P WITH (PARTITIONS=6) AS SELECT * FROM CANX_REASON;

